import java.util.* ;
import java.io.*; 
public class Solution {
    public static int[] ninjaAndSortedArrays(int arr1[], int arr2[], int m, int n) {
        // Write your code here.
        int[] arr3=new int[m+n];
        int k=0;
        for(int i=0;i<m;i++){
            arr3[k++]=arr1[i];
        }
        for(int j=0;j<n;j++){
            arr3[k++]=arr2[j];
        }
        Arrays.sort(arr3);
        return arr3;    
    }
}
